page {
    background: #f5f5f5;
}

.header {
    position: relative;
    height: 240rpx;
    background: #ff5722;
}

.title {
    padding-top: 40rpx;
    text-align: center;
    font-size: 28rpx;
    line-height: 48rpx;
    color: #fff;
}

.total {
    font-size: 48rpx;
    text-align: center;
    color: #fff;
}

.total text {
    font-size: 72rpx;
    padding-left: 10rpx;
    font-family: Arial;
}

.coupontitle {
    border-bottom: 1rpx solid #dcdcdc;
    display: flex;
    font-size: 28rpx;
}

.coupontitlel {
    width: 250rpx;
    text-align: center;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    height: 100rpx;
}

.coupontitlel view {
    border-bottom: 2rpx solid #fff;
    margin: 0 auto;
    font-size: 24rpx;
}

.coupontitlel.active {
    color: #ff5722;
}

.list {
    display: flex;
    margin-bottom: 24rpx;
    padding: 0 20rpx;
    font-size: 28rpx;
    flex-direction: column;
    background-color: #fff;
}

.list:last-child {
    margin-bottom: 0;
}

.lis {
    display: flex;
    flex: 1;
    padding: 10rpx 20rpx;
    border-bottom: 1rpx solid #e0e0e0;
}

.list image {
    height: 160rpx;
    width: 160rpx;
    border-radius: 10rpx;
}

.list .info {
    flex: 1;
    margin-left: 20rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.list .info .name {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.list .info .price {
    color: #ff5722;
    padding-top: 10rpx;
}

.list .info .price text {
    font-size: 32rpx;
    padding-left: 8rpx;
}

.list .info .time {
    display: flex;
    font-size: 24rpx;
    padding-top: 10rpx;
    color: #aaa;
}

.list .info .time .create {
    flex: 1;
}

.list .info .time text {
    color: #ff5722;
}

.list .order {
    display: flex;
    color: #666;
    padding: 10rpx 20rpx;
    justify-content: center;
    font-size: 24rpx;
}

.list .order .num {
    flex: 1;
}