}

.form-normal {
    background-color: #fff;
    border-radius: 20rpx;
    position: relative;
    box-shadow: 0px 0px 9rpx rgba(0,0,0,0.5);
}

.form-light {
    background-color: #297cd8;
    border-radius: 10rpx;
    position: relative;
    border: 1rpx solid rgba(255,255,255,0.1);
    box-shadow: 0px 0px 5px rgba(35,190,255,0.2) inset;
}

.show-form {
    animation: frames-show-form 0.4s ease;
}

@-webkit-keyframes frames-show-form {
    0% {
        opacity: 0;
        transform: scale(0.2);
    }

    50% {
        opacity: 1;
        transform: scale(1.2);
    }

    75% {
        transform: scale(0.9);
    }

    100% {
        transform: scale(1);
    }
}

@keyframes frames-show-form {
    0% {
        opacity: 0;
        transform: scale(0.2);
    }

    50% {
        opacity: 1;
        transform: scale(1.2);
    }

    75% {
        transform: scale(0.9);
    }

    100% {
        transform: scale(1);
    }
}

.show-form-item {
    animation: frame-show-form-item 0.5s;
}

@-webkit-keyframes frame-show-form-item {
    0% {
        top: -800rpx;
        transform: scale(0.8,1.2);
        transform-origin: 50% 100%;
    }

    40% {
        top: 0px;
        transform: scale(1.2,0.5);
        transform-origin: 50% 100%;
    }

    50% {
        top: 0px;
        transform: scale(1.25,0.45);
        transform-origin: 50% 100%;
    }

    100% {
        top: 0px;
        transform: scale(1,1);
        transform-origin: 50% 100%;
    }
}

@keyframes frame-show-form-item {
    0% {
        top: -800rpx;
        transform: scale(0.8,1.2);
        transform-origin: 50% 100%;
    }

    40% {
        top: 0px;
        transform: scale(1.2,0.5);
        transform-origin: 50% 100%;
    }

    50% {
        top: 0px;
        transform: scale(1.25,0.45);
        transform-origin: 50% 100%;
    }

    100% {
        top: 0px;
        transform: scale(1,1);
        transform-origin: 50% 100%;
    }
}

.heart-bit {
    animation: frame-heart-bit 1s infinite ease-in 2s;
}

@-webkit-keyframes frame-heart-bit {
    0% {
        transform: scale(0.8) rotate(-5deg);
    }

    30% {
        transform: scale(1) rotate(0deg);
    }

    70% {
        transform: scale(1) rotate(0deg);
    }

    100% {
        transform: scale(0.8) rotate(-5deg);
    }
}

@keyframes frame-heart-bit {
    0% {
        transform: scale(0.8) rotate(-5deg);
    }

    30% {
        transform: scale(1) rotate(0deg);
    }

    70% {
        transform: scale(1) rotate(0deg);
    }

    100% {
        transform: scale(0.8) rotate(-5deg);
    }
}

