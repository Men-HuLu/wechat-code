.mycenter-container {
    width: 100%;
    font-family: PingFangSC-Regular,sans-serif;
    overflow: hidden;
}

.my-center-info {
    width: 100%;
}

.my-center-info .personal-details {
    display: flex;
    margin-top: 50rpx;
    width: 100%;
    height: 184rpx;
}

.my-center-info .personal-details .usericon-box {
    margin-left: 36rpx;
    border-radius: 50%;
    width: 132rpx;
    height: 132rpx;
    overflow: hidden;
}

.my-center-info .personal-details .usericon-box .usericon {
    border-radius: 50%;
    width: 100%;
    height: 100%;
    background-image: url(https://cs-op.douyucdn.cn/dyfelocal/act/5acc7c96d51caa4669f8b2de/defaulticon.png?timestamp=1527749111);
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.my-center-info .personal-details .user-details {
    display: flex;
    flex-direction: column;
    margin-left: 36rpx;
}

.my-center-info .personal-details .user-details .user-name {
    height: 66rpx;
    line-height: 66rpx;
    color: #3f3f3f;
    font-size: 32rpx;
    font-weight: bold;
}

.my-center-info .personal-details .user-details .info-num {
    display: flex;
    align-items: center;
    height: 66rpx;
    font-size: 24rpx;
    color: #B8B8B8;
}

.my-center-info .personal-details .user-details .info-num .level {
    display: flex;
    align-items: flex-end;
    margin-right: 30rpx;
}

.my-center-info .personal-details .user-details .info-num .level .level-icon {
    width: 26rpx;
    height: 24rpx;
    margin-right: 5rpx;
}

.my-center-info .personal-details .user-details .info-num .level .info-text {
    height: 25rpx;
    line-height: 25rpx;
}

.my-center-info .personal-details .user-details .info-num .yuchi-wrap {
    display: flex;
    align-items: flex-end;
    margin-right: 30rpx;
}

.my-center-info .personal-details .user-details .info-num .yuchi-wrap .yuchi-icon {
    width: 26rpx;
    height: 24rpx;
    margin-right: 5rpx;
}

.my-center-info .personal-details .user-details .info-num .yuchi-wrap .info-text {
    height: 25rpx;
    line-height: 25rpx;
}

.my-center-info .personal-details .user-details .info-num .yuwan-wrap {
    display: flex;
    align-items: flex-end;
    margin-right: 30rpx;
}

.my-center-info .personal-details .user-details .info-num .yuwan-wrap .yuwan-icon {
    width: 24rpx;
    height: 24rpx;
    margin-right: 5rpx;
}

.my-center-info .personal-details .user-details .info-num .yuwan-wrap .info-text {
    height: 25rpx;
    line-height: 25rpx;
}

.my-center-info .personal-details .userinfo-box {
    width: 100%;
    height: 208rpx;
}

.my-center-info .personal-details .userinfo-box .info {
    float: left;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 33.33333%;
    height: 100%;
}

.my-center-info .personal-details .userinfo-box .info .info-num {
    margin-bottom: 20rpx;
    width: 100%;
    line-height: 44rpx;
    text-align: center;
    font-size: 40rpx;
    color: #3f3f3f;
    font-weight: bold;
}

.my-center-info .personal-details .userinfo-box .info .info-name {
    width: 100%;
    line-height: 28rpx;
    text-align: center;
    font-size: 24rpx;
    color: #ababab;
}

.my-center-info .not-login {
    box-sizing: border-box;
    padding-top: 102rpx;
    width: 100%;
    height: 488rpx;
}

.my-center-info .not-login .default-icon-box {
    margin: 0 auto 40rpx;
    border-radius: 50%;
    width: 148rpx;
    height: 148rpx;
}

.my-center-info .not-login .default-icon-box .default-icon {
    border-radius: 50%;
    width: 100%;
    height: 100%;
}

.my-center-info .not-login .tip-text {
    margin-bottom: 24rpx;
    width: 100%;
    height: 33rpx;
    line-height: 33rpx;
    text-align: center;
    font-size: 24rpx;
    color: #5c5c5c;
}

.my-center-info .not-login .login-btn {
    margin: 0 auto;
    border-radius: 3rpx;
    width: 400rpx;
    height: 72rpx;
    text-align: center;
    line-height: 72rpx;
    font-size: 28rpx;
    color: #fff;
    background: #FF5D23;
}

.my-center-info .line-between {
    width: 100%;
    height: 20rpx;
    background: #fafafa;
}

.my-center-info .yuchi-recharge-box {
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    padding: 0 32rpx 0 36rpx;
    border-bottom: 1px solid #f5f5f5;
    width: 100%;
    height: 100rpx;
}

.my-center-info .yuchi-recharge-box .recharge-title {
    line-height: 32rpx;
    font-size: 28rpx;
    color: #3f3f3f;
}

.my-center-info .yuchi-recharge-box .arrow-icon {
    margin-left: 20rpx;
    width: 13rpx;
    height: 23rpx;
}

.my-center-info .game-giftbag-box {
    position: relative;
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    padding: 0 32rpx 0 36rpx;
    border-bottom: 1px solid #f5f5f5;
    width: 100%;
    height: 100rpx;
}

.my-center-info .game-giftbag-box .game-title {
    line-height: 32rpx;
    font-size: 28rpx;
    color: #3f3f3f;
}

.my-center-info .game-giftbag-box .game-title .red-dot {
    border-radius: 50%;
    margin-right: 10rpx;
    margin-left: -6rpx;
    margin-bottom: 18rpx;
    width: 10rpx;
    height: 10rpx;
    background: #ff5d23;
    display: inline-block;
}

.my-center-info .game-giftbag-box .giftbag-intro {
    position: absolute;
    right: 59rpx;
    top: 50%;
    transform: translateY(-50%);
    display: flex;
    align-items: center;
    height: 36rpx;
}

.my-center-info .game-giftbag-box .giftbag-intro .giftbag-text {
    height: 100%;
    line-height: 36rpx;
    font-size: 24rpx;
    color: #5c5c5c;
}

.my-center-info .game-giftbag-box .giftbag-intro .giftbag-icon {
    width: 60rpx;
    height: 60rpx;
    margin-left: 10rpx;
}

.my-center-info .game-giftbag-box .arrow-icon {
    margin-left: 20rpx;
    width: 13rpx;
    height: 23rpx;
}

.my-center-info .contact-service {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 100%;
    height: 100rpx;
    box-sizing: border-box;
    padding: 0 32rpx 0 36rpx;
    border-bottom: 1px solid #f5f5f5;
    background-color: transparent;
}

.my-center-info .contact-service::after {
    border: none;
}

.my-center-info .contact-service .service-title {
    font-size: 28rpx;
    color: #3f3f3f;
}

.my-center-info .contact-service .arrow-icon {
    width: 13rpx;
    height: 23rpx;
}

.my-center-info .download-app {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 100%;
    height: 100rpx;
    box-sizing: border-box;
    padding: 0 32rpx 0 36rpx;
    border-bottom: 1px solid #f5f5f5;
}

.my-center-info .download-app .download-title {
    font-size: 28rpx;
    color: #3f3f3f;
}

.my-center-info .download-app .arrow-icon {
    width: 13rpx;
    height: 23rpx;
}

.my-center-info .watch-record-wrap {
    border-bottom: 1px solid #f5f5f5;
    width: 100%;
}

.my-center-info .watch-record-wrap .watch-record-title {
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    padding-left: 36rpx;
    width: 100%;
    height: 100rpx;
}

.my-center-info .watch-record-wrap .watch-record-title .wr-title-name {
    line-height: 32rpx;
    font-size: 28rpx;
    color: #3f3f3f;
}

.my-center-info .watch-record-wrap .watch-record-title .wr-control-btn {
    padding: 0 32rpx;
    line-height: 28rpx;
    font-size: 24rpx;
    color: #999;
}

.my-center-info .watch-record-wrap .watch-record-content {
    box-sizing: border-box;
    padding-left: 24rpx;
    width: 100%;
    height: 250rpx;
    font-size: 0;
    white-space: nowrap;
    overflow: hidden;
}

.my-center-info .watch-record-wrap .watch-record-content .record-content-box {
    display: inline-block;
    vertical-align: middle;
    margin-right: 12rpx;
    width: 264rpx;
    height: 100%;
}

.my-center-info .watch-record-wrap .watch-record-content .record-content-box .content-main {
    position: relative;
    width: 100%;
    height: 150rpx;
}

.my-center-info .watch-record-wrap .watch-record-content .record-content-box .content-main .zhanweitu {
    width: 100%;
    height: 100%;
}

.my-center-info .watch-record-wrap .watch-record-content .record-content-box .content-main .cover-pic {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1;
    width: 100%;
    height: 100%;
}

.my-center-info .watch-record-wrap .watch-record-content .record-content-box .content-main .content-type {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 5;
    width: 64rpx;
    height: 32rpx;
    line-height: 32rpx;
    text-align: center;
    font-size: 20rpx;
    color: #fff;
    background: #f70;
}

.my-center-info .watch-record-wrap .watch-record-content .record-content-box .content-main .content-type.videotype {
    background: #0084FF;
}

.my-center-info .watch-record-wrap .watch-record-content .record-content-box .live-des-box {
    box-sizing: border-box;
    padding: 12rpx 10rpx 0;
    width: 100%;
}

.my-center-info .watch-record-wrap .watch-record-content .record-content-box .live-des-box .live-title {
    width: 100%;
    height: 32rpx;
    line-height: 32rpx;
    font-size: 28rpx;
    color: #333;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.my-center-info .watch-record-wrap .watch-record-content .record-content-box .live-des-box .username {
    margin-top: 6rpx;
    width: 100%;
    height: 28rpx;
    line-height: 28rpx;
    font-size: 24rpx;
    color: #999;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.my-center-info .watch-record-wrap .watch-record-content .record-content-box .video-title {
    box-sizing: border-box;
    padding: 12rpx 10rpx 0;
    width: 100%;
    height: 74rpx;
    line-height: 32rpx;
    font-size: 28rpx;
    color: #333;
    word-break: break-all;
    white-space: normal;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.my-center-info .account-management {
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
