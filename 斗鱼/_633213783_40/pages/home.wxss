.home-container {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
}

.home-container .home-container-content {
    overflow-x: hidden;
    -webkit-overflow-scrolling: touch;
    width: 100%;
    margin-top: 76rpx;
}

.home-container .scroll-view-wrap {
    height: 76rpx;
    background-color: #ff5d23;
    position: fixed;
    top: 0;
    left: 0;
    border-bottom: none;
}

.home-container .scroll-view-wrap .scroll-view-catalog {
    height: 76rpx;
}

.home-container .scroll-view-wrap .scroll-view-catalog .scroll-view-item {
    padding: 19rpx 23rpx;
    font-size: 29rpx;
    color: #fff;
    font-weight: normal;
}

.home-container .scroll-view-wrap .scroll-view-catalog .active-catalog {
    font-weight: bold;
}

.room-item .room-item-banner {
    position: relative;
    width: 370rpx;
    height: 210rpx;
}

.room-item .room-item-banner .room-default-image {
    width: 370rpx;
    height: 210rpx;
    z-index: 5;
}

.room-item .room-item-banner .room-item-image {
    width: 370rpx;
    height: 210rpx;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    z-index: 10;
}

.room-item .room-item-banner .room-item-banner-footer {
    position: absolute;
    color: #fff;
    font-size: 22rpx;
    width: 100%;
    height: 88rpx;
    box-sizing: border-box;
    bottom: 0rpx;
    background: linear-gradient(to bottom,transparent,rgba(0,0,0,.3));
    z-index: 10;
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
    padding-bottom: 8rpx;
}

.room-item .room-item-banner .room-item-banner-footer .room-item-footer-name {
    position: relative;
    left: 14rpx;
    float: left;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    flex: 1;
    padding-right: 10rpx;
}

.room-item .room-item-banner .room-item-banner-footer .room-item-footer-nums {
    position: relative;
    right: 18rpx;
    margin-left: 25rpx;
    float: right;
}

.room-item .room-item-banner .room-item-banner-footer .room-item-footer-nums::before {
    content: '';
    float: left;
    height: 16rpx;
    width: 15rpx;
    margin-top: 10rpx;
    margin-right: 8rpx;
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: 100% 100%;
}

.room-item .room-item-info {
    width: 370rpx!important;
    height: 60rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.room-item .room-item-info text {
    padding-left: 14rpx;
    font-size: 28rpx;
    color: #3f3f3f;
}

.yz .room-item-banner {
    margin-bottom: 10rpx;
}

.yz .room-default-image,.yz .room-item-banner {
    width: 370rpx!important;
    height: 370rpx!important;
}

.yz .room-item-image {
    width: 370rpx!important;
    height: 370rpx!important;
}

.room-list {
    width: 100%;
    background-color: white;
}

