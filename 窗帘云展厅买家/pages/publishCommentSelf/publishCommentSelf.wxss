.inputbox {
    width: 100%;
    border-top: 1px solid #f4f4f4;
    height: 104rpx;
    line-height: 104rpx;
    font-size: 30rpx;
}

.content_w {
    width: 100%;
    padding: 0 24rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    height: 104rpx;
    line-height: 104rpx;
    font-size: 30rpx;
}

picker .picker {
    font-size: 30rpx;
    color: #666666;
}

.section {
    position: relative;
    margin-top: 34rpx;
}

.ico_picker {
    position: absolute;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
}

.publishBtn {
    background: #46a6f0!important;
}

.publishActive {
    background: #ccc!important;
}