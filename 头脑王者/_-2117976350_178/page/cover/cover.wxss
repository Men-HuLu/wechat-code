.newFrame-arrow {
    animation: frame-newFrame-arrow 0.4s infinite alternate;
}

@-webkit-keyframes frame-newFrame-arrow {
    0% {
        transform: translate3d(-5px,-5px,0);
    }

    100% {
        transform: translate3d(0px,0px,0);
    }
}

@keyframes frame-newFrame-arrow {
    0% {
        transform: translate3d(-5px,-5px,0);
    }

    100% {
        transform: translate3d(0px,0px,0);
    }
}