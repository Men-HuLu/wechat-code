.bg,.my-wrap,image {
    width: 100%;
    height: 100%;
}

.my-head {
    position: relative;
    z-index: 1;
    width: 100%;
    height: 224rpx;
}

.user-info {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    padding-top: 20rpx;
    text-align: center;
}

.user-icon {
    width: 120rpx;
    height: 120rpx;
    line-height: 120rpx;
    margin: 0 auto;
    background: #000;
    border-radius: 50%;
    color: #fde23d;
}

.user-icon,.user-name {
    position: relative;
    font-size: 32rpx;
}

.user-name {
    padding: 14rpx 200rpx;
    height: 36rpx;
    line-height: 36rpx;
    text-align: center;
    color: #333;
}

.user-name .name {
    position: relative;
    overflow: hidden;
    display: inline-block;
    width: auto;
    max-width: 250rpx;
    padding: 0 50rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.user-name .sex {
    top: 0;
    display: inline-block;
    width: 36rpx;
    margin-left: 16rpx;
    vertical-align: bottom;
}

.feedback,.user-name .sex {
    position: absolute;
    right: 0;
    height: 36rpx;
}

.feedback {
    display: flex;
    top: 14rpx;
    width: 200rpx;
    justify-content: center;
    font-size: 24rpx;
}

.feedback image {
    width: 26rpx;
    height: 26rpx;
    margin: 5rpx 3rpx 0 0;
}

button {
    font-size: 24rpx;
    display: flex;
    line-height: 36rpx;
}

.button-hover,button,button:active,button:hover {
    background: none;
    font-weight: 400;
}

button:after {
    border: 0;
}

.my-tab-head {
    position: relative;
    z-index: 1;
    width: 100%;
    height: 100rpx;
    margin-top: -24rpx;
}

.my-tab-btn {
    position: relative;
    width: 375rpx;
    padding: 44rpx 0 20rpx;
    display: inline-block;
    text-align: center;
    font-size: 28rpx;
    color: #666;
}

.my-tab-btn.active {
    color: #f5a623;
}

.my-tab-btn.active:after {
    height: 4rpx;
    width: 54rpx;
    transform: translateX(-50%);
    bottom: 10rpx;
    border-radius: 16rpx;
    background: #f5a623;
}

.my-tab-btn.active:after,.my-tab-btn.reddot:before {
    content: "";
    display: block;
    position: absolute;
    left: 50%;
}

.my-tab-btn.reddot:before {
    width: 10px;
    height: 10px;
    border-radius: 5px;
    top: 35rpx;
    margin-left: 44rpx;
    background: #f43530;
}

.my-tab-body {
    box-sizing: border-box;
    position: absolute;
    z-index: 0;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    padding-top: 300rpx;
}

.my-tab-body .scroll-view {
    height: 100%;
}

.my-list-wrap {
    padding: 16rpx 0;
}

.my-list {
    display: flex;
    justify-content: space-between;
    padding: 6rpx 12rpx;
}

.my-list.active {
    background: #eee;
}

.my-list .topic-wrap {
    position: relative;
    padding: 10rpx 20rpx;
}

.my-list .topic-wrap .pic {
    position: relative;
    z-index: 1;
    width: 248rpx;
}

.my-list .flag {
    position: absolute;
    z-index: 2;
    left: 12rpx;
    top: 10rpx;
    width: 90rpx;
    height: 44rpx;
    line-height: 44rpx;
    text-align: center;
    font-size: 24rpx;
}

.my-list .triangle {
    position: absolute;
    z-index: 0;
    left: 16rpx;
    top: 48rpx;
    width: 0;
    height: 0;
    border-top: 8rpx solid transparent;
    border-bottom: 8rpx solid transparent;
    border-left: 8rpx solid #db3b3b;
    transform: rotate(-45deg);
}

.my-list .flag text {
    position: relative;
    display: block;
    z-index: 1;
    background: #fa7261;
    color: #fff;
    border-top-right-radius: 44rpx;
    border-bottom-right-radius: 44rpx;
}

.my-list .comic-wrap {
    padding: 10rpx 0;
    width: 438rpx;
}

.my-list .comic-wrap,.my-list .info-wrap {
    display: flex;
    justify-content: space-between;
}

.my-list .info-wrap {
    flex-direction: column;
    width: 100%;
    color: #999;
    font-size: 24rpx;
    overflow: hidden;
}

.my-list .info-tit {
    line-height: 45rpx;
    font-size: 32rpx;
    color: #181818;
}

.my-list .info-a .txt-a,.my-list .info-c,.my-list .info-tit {
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.my-list .info-a {
    display: flex;
    margin-top: 30rpx;
}

.my-list .info-a .txt-b {
    color: #e6e6e6;
    text-indent: .5em;
    white-space: nowrap;
}

.my-list .info-b {
    line-height: 24rpx;
}

.my-list .info-b .num {
    color: #ff751a;
}

.my-list .cotinued-wrap {
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
    padding: 0 20rpx;
}

.my-list .cotinued-wrap view {
    width: 90rpx;
    height: 48rpx;
    line-height: 48rpx;
    border-radius: 48rpx;
    border: 1rpx solid #ffba15;
    font-size: 24rpx;
    color: #ffba15;
    text-align: center;
}