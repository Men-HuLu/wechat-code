        <view class="center center2" style="position:absolute;right:18rpx;bottom:38rpx;width:154rpx;height:44rpx;">
            <image src="https://question-resource-wscdn.hortorgames.com/image/new_skin/theme/theme_{{subType}}/button_accept.png?v=3" style="width:100%;height:100%;"></image>
        </view>
        <view style="position: absolute;right:18rpx;bottom:12rpx;font-size: 20rpx;line-height:20rpx;color: #{{fontColor?fontColor:'FFFFFF'}};white-space: nowrap;">{{textCountdown}}</view>
    </view>
</template>
