.liveExpiredView {
    visibility: hidden;
}

.liveExpiredView_background {
    width: 100%;
    height: 100%;
    background-color: #000;
    opacity: 0.8;
}

.liveExpiredViewTitle {
    position: absolute;
    color: #4670CD;
    font-size: 36rpx;
    text-align: left;
    line-height: 55rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 495rpx;
    top: 75rpx;
    left: 0;
    right: 0;
    margin: auto;
    text-shadow: 0rpx 0rpx 16rpx #006bfd;
}

.liveExpiredViewContent {
    position: absolute;
    color: #51bafe;
    font-size: 32rpx;
    text-align: left;
    line-height: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 495rpx;
    top: 140rpx;
    left: 0;
    right: 0;
    margin: auto;
    text-shadow: 0rpx 0rpx 16rpx #006bfd;
}