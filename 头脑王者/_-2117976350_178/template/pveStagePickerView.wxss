.starImg {
    position: absolute;
    display: flex;
    flex: none;
    width: 45rpx;
    height: 45rpx;
}

.starImg.addStar {
    animation: addStar 0.8s;
}

.starImg.subStar {
    animation: subStar 1s;
}

.starImg.allClearStar {
    animation: allClearStar 0.8s;
}

@-webkit-keyframes addStar {
    0% {
        transform: scale(1.5,1.5);
    }

    50% {
        transform: scale(2,2);
    }

    60% {
        transform: scale(1,1);
    }

    70% {
        transform: scale(1.4,1.4);
    }

    75% {
        transform: scale(1,1);
    }

    80% {
        transform: scale(1.3,1.3);
    }

    85% {
        transform: scale(1,1);
    }

    100% {
        transform: scale(1,1);
    }
}

@keyframes addStar {
    0% {
        transform: scale(1.5,1.5);
    }

    50% {
        transform: scale(2,2);
    }

    60% {
        transform: scale(1,1);
    }

    70% {
        transform: scale(1.4,1.4);
    }

    75% {
        transform: scale(1,1);
    }

    80% {
        transform: scale(1.3,1.3);
    }

    85% {
        transform: scale(1,1);
    }

    100% {
        transform: scale(1,1);
    }
}

@-webkit-keyframes subStar {
    0% {
        transform: scale(1,1);
    }

    50% {
        transform: scale(1.5,1.5);
    }

    80% {
        transform: scale(0,0);
    }

    100% {
        transform: scale(0,0);
    }
}

@keyframes subStar {
    0% {
        transform: scale(1,1);
    }

    50% {
        transform: scale(1.5,1.5);
    }

    80% {
        transform: scale(0,0);
    }

    100% {
        transform: scale(0,0);
    }
}

@-webkit-keyframes allClearStar {
    0% {
        transform: scale(1,1);
    }

    50% {
        transform: scale(1.5,1.5);
    }

    100% {
        transform: scale(1,1);
    }
}

@keyframes allClearStar {
    0% {
        transform: scale(1,1);
    }

    50% {
        transform: scale(1.5,1.5);
    }

    100% {
        transform: scale(1,1);
    }
}

.lightEff {
    position: absolute;
    display: flex;
    flex: none;
    width: 82rpx;
    height: 86rpx;
    left: -14rpx;
    top: -19rpx;
    opacity: 0;
}

.lightEff.addStar {
    animation: lightEff_addStar 0.8s;
}

.lightEff.subStar {
    animation: lightEff_subStar 1s;
}

@-webkit-keyframes lightEff_addStar {
    59% {
        opacity: 0;
        transform: scale(0,0);
    }

    60% {
        opacity: 0;
        transform: scale(1,1);
    }

    80% {
        opacity: 0.5;
        transform: scale(2,2);
    }

    100% {
        opacity: 0;
        transform: scale(3,3);
    }
}

@keyframes lightEff_addStar {
    59% {
        opacity: 0;
        transform: scale(0,0);
    }

    60% {
        opacity: 0;
        transform: scale(1,1);
    }

    80% {
        opacity: 0.5;
        transform: scale(2,2);
    }

    100% {
        opacity: 0;
        transform: scale(3,3);
    }
}

@-webkit-keyframes lightEff_subStar {
    0% {
        opacity: 0;
        transform: scale(0,0);
    }

    30% {
        opacity: 0.5;
        transform: scale(1.5,1.5);
    }

    100% {
        opacity: 0;
        transform: scale(0,0);
    }
}

@keyframes lightEff_subStar {
    0% {
        opacity: 0;
        transform: scale(0,0);
    }

    30% {
        opacity: 0.5;
        transform: scale(1.5,1.5);
    }

    100% {
        opacity: 0;
        transform: scale(0,0);
    }
}

.startCell {
    display: flex;
    flex: none;
    width: 45rpx;
    height: 45rpx;
    position: relative;
    padding-bottom: 2rpx;
}

.lockAni {
    animation: lockAni_allClearStar 0.8s;
}

@-webkit-keyframes lockAni_allClearStar {
    0% {
        transform: scale(1,1);
    }

    30% {
        transform: scale(1.5,1.5);
    }

    100% {
        transform: scale(0,0);
    }
}

@keyframes lockAni_allClearStar {
    0% {
        transform: scale(1,1);
    }

    30% {
        transform: scale(1.5,1.5);
    }

    100% {
        transform: scale(0,0);
    }
}

.bannerAni {
    animation: bannerAni_allClearStar 0.8s;
}

@-webkit-keyframes bannerAni_allClearStar {
    0% {
        transform: brightness(0.5,0.5);
    }

    60% {
        filter: brightness(1.2);
    }

    100% {
        transform: brightness(1,1);
    }
}

@keyframes bannerAni_allClearStar {
    0% {
        transform: brightness(0.5,0.5);
    }

    60% {
        filter: brightness(1.2);
    }

    100% {
        transform: brightness(1,1);
    }
}

.avatarOnly-tiers {
    position: absolute;
    top: calc(50% - 130% / 2);
    left: calc(50% - 130% / 2);
    width: 130%;
    height: 130%;
    flex: none;
}