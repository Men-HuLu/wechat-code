    width: 100%;
    height: 100rpx;
}

.my-center-info .account-management .am-title {
    line-height: 32rpx;
    font-size: 28rpx;
    color: #3f3f3f;
}

.my-center-info .account-management .arrow-icon {
    margin-left: 20rpx;
    width: 13rpx;
    height: 23rpx;
}