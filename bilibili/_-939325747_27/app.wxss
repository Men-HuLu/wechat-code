.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
    background: white;
}

.clearfix::after {
    content: "";
    display: block;
    height: 0;
    clear: both;
}