.tab {
    width: 240rpx;
    text-align: center;
    padding: 20rpx 0;
}

.tab_active {
    border-bottom: 4rpx solid #3fa2ef;
}

.tab text {
    display: block;
    line-height: 1;
}

.tab text:nth-of-type(1) {
    font-size: 24rpx;
    color: #999999;
}

.tab text:nth-last-of-type(1) {
    font-size: 28rpx;
    color: #333333;
}

.tab_active .text:nth-last-of-type(1) {
    color: #3fa2ef!important;
}

#visitorList {
    padding: 0 24rpx;
}

.visitorListBox {
    padding: 20rpx 0;
}

.visitorListUserName {
    font-size: 28rpx;
    color: #333333;
    display: block;
    line-height: 1;
    width: 240rpx;
}

.visitorListUserNum {
    font-size: 26rpx;
    color: #333333;
    display: block;
    line-height: 1;
    margin-top: 18rpx;
}

.visitorListTime {
    font-size: 24rpx;
    color: #999999;
    line-height: 1;
    display: block;
    margin-right: 12rpx;
}

.modal {
    position: absolute;
    height: 100%;
    width: 100%;
    background: #000;
    opacity: .1;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: 2;
}

.modalFrame {
    width: 580rpx;
    height: 350rpx;
    border-radius: 10rpx;
    background: #fff;
    box-sizing: border-box;
    padding: 30rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -172rpx;
    margin-left: -290rpx;
    z-index: 99;
}

.modalFrame .title {
    font-size: 36rpx;
    color: #333333;
    line-height: 1;
    margin-top: 8rpx;
}

.modalFrame .txt {
    font-size: 28rpx;
    margin-top: 32rpx;
    color: #666666;
    line-height: 1;
    margin-bottom: 84rpx;
}

.modalFrame .btn {
    display: flex;
    justify-content: flex-end;
}

.modalFrame .btn view {
    font-size: 30rpx;
    line-height: 1;
}

.modalFrame .btn view:nth-of-type(1) {
    margin-right: 52rpx;
    color: #333333;
}

.modalFrame .btn view:nth-last-of-type(1) {
    color: #369eef;
}

.noData {
    width: 100%;
    text-align: center;
    line-height: 1;
    color: #333333;
    font-size: 34rpx;
    margin-top: 100rpx;
}