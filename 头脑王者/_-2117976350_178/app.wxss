.item-shine {
    animation: frame-item-shaine 2s infinite linear;
}

@-webkit-keyframes frame-item-shaine {
    0% {
        filter: brightness(0.8);
    }

    50% {
        filter: brightness(1.2);
    }

    100% {
        filter: brightness(0.8);
    }
}

@keyframes frame-item-shaine {
    0% {
        filter: brightness(0.8);
    }

    50% {
