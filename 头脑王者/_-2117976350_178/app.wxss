page {
    background-color: #4E84E5;
    height: 100%;
    width: 100%;
    overflow: hidden;
    color: #fefefe;
    position: absolute;
    top: 0px;
    left: 0px;
    font-weight: bold;
}

button {
    background-color: rgba(0,0,0,0);
    color: #fefefe;
    border: 4rpx solid #fff;
    width: 200rpx;
    height: 74rpx;
    margin: initial;
    padding: initial;
    box-sizing: border-box;
    border-radius: 10rpx;
    font-size: 34rpx;
    text-align: center;
    line-height: normal;
    overflow: hidden;
    text-decoration: none;
    display: flex;
    align-content: center;
    align-items: center;
    flex-direction: row;
    justify-content: center;
}

button:active {
    background-color: #fff;
    color: #2b0660;
}

.button-hover {
    background-color: #fff;
    color: #2b0660;
}

button::after {
    content: none;
}

button[disabled][type="default"],button[disabled]:not([type]) {
    background-color: rgba(0,0,0,0);
    color: #55565b;
    border: 4rpx solid #55565b;
}

