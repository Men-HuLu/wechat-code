.mainView {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0px;
    left: 0px;
}

.beginBtn {
    background: #00E1BD;
    box-shadow: 0 2rpx 4rpx 0 rgba(0,0,0,0.34);
    border-radius: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 56rpx;
    color: #FFFFFF;
    letter-spacing: 4rpx;
    text-shadow: 0 2rpx 4rpx rgba(0,0,0,0.50);
    width: 440rpx;
    height: 114rpx;
    position: absolute;
    left: calc(50% - 440rpx / 2);
    bottom: 180rpx;
}

.flagImg {
    width: 242rpx;
    height: 165rpx;
    position: absolute;
    top: 112rpx;
    left: calc(50% - 242rpx / 2);
}

.labelView {
    width: 385rpx;
    height: 67rpx;
    position: absolute;
    left: calc(50% - 385rpx / 2);
    top: 360rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    line-height: 67rpx;
    font-size: 48rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    text-shadow: 0 1rpx 2rpx rgba(0,0,0,0.50);
    white-space: nowrap;
}

.labelView2 {
    width: 288rpx;
    height: 45rpx;
    position: absolute;
    left: calc(50% - 288rpx / 2);
    top: 425rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    line-height: 45rpx;
    font-size: 32rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    text-shadow: 0 1rpx 2rpx rgba(0,0,0,0.50);
    white-space: nowrap;
}

.dotMainView {
    width: 540rpx;
    position: absolute;
    left: calc(50% - 540rpx / 2);
    top: 550rpx;
    display: flex;
    justify-content: center;
    align-items: flex-end;
}

.dotView {
    width: 45rpx;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    flex-direction: column;
}

.dotImg {
    width: 35rpx;
    height: 35rpx;
    flex: none;
}

.dotLabel {
    flex: none;
    width: 23rpx;
    height: 40rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    line-height: 40rpx;
    font-size: 24rpx;
    color: #7FEEFF;
    letter-spacing: 0;
    white-space: nowrap;
}

.dotLabel2 {
    flex: none;
    width: 23rpx;
    height: 40rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    line-height: 40rpx;
    font-weight: bold;
    font-size: 34rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    white-space: nowrap;
}

.dotLabel3 {
    flex: none;
    width: 47rpx;
    height: 29rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    line-height: 29rpx;
    font-size: 24rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    text-shadow: 0 1rpx 2rpx rgba(0,0,0,0.50);
    white-space: nowrap;
}

.dotIcon1 {
    flex: none;
    width: 40rpx;
    height: 40rpx;
    margin: 0rpx 0rpx 5rpx 0rpx;
}

.dotIcon2 {
    flex: none;
    width: 50rpx;
    height: 50rpx;
    margin: 5rpx;
    margin: 0rpx 0rpx 5rpx 0rpx;
}

.dotIcon3 {
    flex: none;
    width: 60rpx;
    height: 60rpx;
    margin: 0rpx 0rpx 5rpx 0rpx;
}